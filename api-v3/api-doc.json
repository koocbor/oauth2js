{
  "openapi": "3.0.1",
  "info": {
    "title": "Teambuildr",
    "version": "3.0.0"
  },
  "paths": {
    "/me": {
      "get": {
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/user"
                }
              }
            }
          }
        },
        "security": [
          {
            "OAuth": []
          }
        ]
      }
    }
  },
  "components": {
    "schemas": {
      "user": {
        "properties": {
          "username": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        }
      }
    },
    "securitySchemes": {
      "OAuth": {
        "type": "oauth2",
        "description": "Password Flow",
        "flows": {
          "password": {}
        }
      }
    }
  }
}